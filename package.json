  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/kitware/itk-vtk-viewer/issues"
  },
  "homepage": "https://kitware.github.io/itk-vtk-viewer/",
  "main": "./dist/itkVtkViewer.js",
  "dependencies": {
    "@thewtex/iconselect.js": "^2.1.2",
    "@xstate/inspect": "^0.1.1",
    "axios": "^0.19.2",
    "commander": "^2.20.3",
    "css-element-queries": "^1.2.3",
    "curry": "^1.2.0",
    "eventemitter3": "^4.0.4",
    "express": "^4.17.1",
    "itk": "^13.2.0",
    "mobx": "^5.15.4",
    "mousetrap": "^1.6.5",
    "open": "^6.4.0",
    "promise-file-reader": "^1.0.2",
    "regenerator-runtime": "^0.13.5",
    "vtk.js": "^14.11.4",
    "webworker-promise": "^0.4.2",
    "xstate": "^4.13.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/runtime": "^7.9.6",
    "babel-plugin-istanbul": "^5.2.0",
    "babel-preset-mobx": "^2.0.0",
    "copy-webpack-plugin": "^4.5.1",
    "husky": "^4.2.3",
    "imjoy-core": "^0.13.37",
    "karma": "^5.0.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^1.3.0",
    "karma-junit-reporter": "^2.0.1",
    "karma-tap": "^4.2.0",
    "karma-tap-pretty-reporter": "^4.2.0",
    "karma-webpack": "4.0.2",
    "kw-doc": "^1.3.1",
    "kw-web-suite": "^8.0.0",
    "lint-staged": "^10.2.2",
    "ndarray": "^1.0.19",
    "prettier": "^1.19.1",
    "puppeteer": "^2.1.1",
    "request": "^2.88.2",
    "resemblejs": "^3.2.4",
    "tap-markdown": "^1.2.1",
    "tap-spec": "^5.0.0",
    "tape": "^4.13.2",
    "tape-catch": "^1.0.6",
    "webpack-dev-server": "^3.11.0",
    "webpackbar": "^3.2.0",
    "workbox-build": "^4.3.1",
    "workbox-webpack-plugin": "^4.3.1",
    "worker-loader": "^2.0.0"
  },
  "scripts": {
    "doc": "kw-doc -c ./doc/config.js",
    "doc:www": "kw-doc -c ./doc/config.js -s",
    "doc:publish": "kw-doc -c ./doc/config.js -mp",
    "build": "webpack --progress --colors --mode production",
    "build:debug": "webpack --progress --colors --display-modules --mode development",
    "build:release": "npm run build -- -p",
    "bundle": "StandaloneHTML ./dist/index.html ./dist/ItkVtkViewer.html",
    "commit": "git cz",
    "format": "prettier --write src/UserInterface/**/*.js src/*.js",
    "start": "webpack-dev-server --mode development --content-base ./dist/ --watch-content-base --open",
    "semantic-release": "semantic-release",
    "test": "karma start ./karma.conf.js --browsers Chrome,Firefox",
    "test:debug": "karma start ./karma.conf.js --no-single-run"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "bin": {
    "itk-vtk-viewer": "./bin/itk-vtk-viewer-cli.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "prettier --write"
  }
}
